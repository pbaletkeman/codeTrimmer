{
  "codetrimmer": {
    "include": "*.{py,js,java}",
    "exclude": "node_modules/**,*.min.js",
    "include-hidden": false,
    "follow-symlinks": false,
    "max-consecutive-blank-lines": 2,
    "ensure-final-newline": true,
    "trim-trailing-whitespace": true,
    "max-file-size": 5242880,
    "max-files": 100,
    "no-limits": false,
    "dry-run": false,
    "create-backups": true,
    "fail-fast": false,
    "verbose": false,
    "quiet": false,
    "no-color": false
  }
}
